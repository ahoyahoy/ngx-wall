{
    "name": "ngx-wall",
    "version": "1.1.17",
    "main": "dist/ngx-wall/bundles/ngx-wall.umd.js",
    "module": "dist/ngx-wall/fesm5/ngx-wall.js",
    "es2015": "dist/ngx-wall/fesm2015/ngx-wall.js",
    "esm5": "dist/ngx-wall/esm5/ngx-wall.js",
    "esm2015": "dist/ngx-wall/esm2015/ngx-wall.js",
    "fesm5": "dist/ngx-wall/fesm5/ngx-wall.js",
    "fesm2015": "dist/ngx-wall/fesm2015/ngx-wall.js",
    "typings": "dist/ngx-wall/ngx-wall.d.ts",
    "metadata": "dist/ngx-wall/ngx-wall.metadata.json",
    "repository": {
        "url": "https://github.com/vm-mishchenko/ngx-wall.git",
        "type": "git"
    },
    "scripts": {
        "start": "ng serve",
        "build": "ng build ngx-wall",
        "build:prod": "ng build ngx-wall --prod",
        "build:watch": "ng build ngx-wall --watch",
        "build-for-origin": "run-p build:watch copy:scss:watch",
        "copy:scss": "cpx \"projects/ngx-wall/src/lib/resources/**/*\" \"dist/ngx-wall\"",
        "copy:scss:watch": "cpx -w \"projects/ngx-wall/src/lib/resources/**/*\" \"dist/ngx-wall\"",
        "release": "npm run test:once && ng build ngx-wall --prod && npm run copy:scss",
        "gh": "ng build --stats-json --prod --base-href /ngx-wall/",
        "test": "ng test ngx-wall",
        "test:once": "ng test ngx-wall --watch=false --code-coverage",
        "test:cli": "ng test ngx-wall --watch=false  --code-coverage --no-progress --browsers=ChromeNoSandbox",
        "lint": "ng lint ngx-wall",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^7.2.15",
        "@angular/core": "^11.0.5",
        "@angular/cdk": "^7.3.7",
        "@angular/compiler": "^7.2.15",
        "@angular/animations": "^7.2.15",
        "@angular/material": "^7.3.7",
        "@angular/forms": "^7.2.15",
        "@angular/http": "^7.2.15",
        "@angular/platform-browser": "^7.2.15",
        "@angular/platform-browser-dynamic": "^7.2.15",
        "@angular/router": "^7.2.15",
        "core-js": "^3.1.2",
        "rxjs": "^6.5.2",
        "zone.js": "^0.9.1",
        "codemirror": "5.47.0",
        "ngx-sticky-modal": "^0.0.4",
        "bootstrap": "^4.3.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.9",
        "@angular-devkit/build-ng-packagr": "^0.13.9",
        "@angular/cli": "^7.3.9",
        "@angular/compiler-cli": "^7.2.15",
        "@angular/language-service": "^7.2.15",
        "@types/jasmine": "~3.3.12",
        "@types/jasminewd2": "~2.0.6",
        "@types/node": "~12.0.2",
        "codelyzer": "~5.0.1",
        "coveralls": "^3.0.3",
        "cpx": "1.5.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ng-packagr": "^5.2.0",
        "npm-run-all": "^4.1.5",
        "protractor": "~5.4.2",
        "ts-node": "~8.1.0",
        "tsickle": "0.35.0",
        "tslib": "^1.9.3",
        "tslint": "~5.16.0",
        "typescript": "~3.2.4"
    }
}
